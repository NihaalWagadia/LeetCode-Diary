int[][] rotateImage(int[][] a) {
for(int i=0; i<a.length; i++){
    for(int j=i; j<a[0].length; j++){
        if(i!=j){
            int temp = a[i][j];
            a[i][j]=a[j][i];
            a[j][i]=temp;
        }
    }
}
for(int i=0; i<a.length; i++){
    int[]temp = a[i];
    for(int first=0, last=temp.length-1; first<temp.length/2; first++ ){
        int x = temp[first];
        temp[first]=temp[last];
        temp[last]=x;
        last--;
    }
}
return a;
}
