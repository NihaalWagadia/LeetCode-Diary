class Solution {
    public int climbStairs(int n) {
        int []arr = new int[n];
        if(n==1)return 1;
        if(n==2)return 2;
        arr[0]=1;
        arr[1]=2;
        int i=2;
        while(i<n){
            arr[i]=arr[i-1]+arr[i-2];
            i++;
        }
        return arr[n-1];
    }
}
