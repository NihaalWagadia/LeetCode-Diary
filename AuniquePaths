Breaker:- Number of ways you can reach each cell

class Solution {
    public int uniquePaths(int m, int n) {
        int[][] trace = new int[n][m];
        for(int i=0; i<n; i++){
            trace[i][0]=1;
        }
        
        for(int i=0; i<m; i++){
            trace[0][i]=1;
        }
        
        for(int i=1; i<n; i++){
            for(int j=1; j<m; j++){
                trace[i][j] = trace[i-1][j]+trace[i][j-1];
            }
        }
        return trace[n-1][m-1];
    }
}
